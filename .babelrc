{
    "presets": ["react-native"],
    "plugins": [
    [
        "module-resolver",
        {
            "root": ["./"]
        }
    ]
    ],
    "env": {
        "development": {
            "plugins": [
                [
                    "transform-define",
                    {
                        "SENTRY_ENABLED": false
                    }
                ]
            ]
        },
        "production": {
            "plugins": [
                [
                    "transform-define",
                    {
                        "SENTRY_ENABLED": true
                    }
                ],
                "transform-remove-console"
            ]
        }
    }
}
